<h1>Create a new Blog</h1>
<%= form_with model: @blogpost do |form|%>
<%= form.label :title%>
<%= form.text_field :tilte%>
<br>
<br>
<%= form.label :content%>
<%= form.text_field :content%>
<br>
<br>
<%= form.submit 'Post Blog'%>
<%= form.submit 'Reset'%>
<% end %>
<br>
<br>
<h1><%=link_to 'Go to Homepage','/blogposts'%></h1>
